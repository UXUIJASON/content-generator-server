{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://content-generator-omega.vercel.app"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "OPTIONS,GET,POST,PUT,DELETE"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "*"
          }
        ]
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/api/$1" }
    ]
  }